package bladibla;

import java.util.Random;

public class Test implements Runnable{


	public void run(){
		
	}
	
	public static void main(String[] args){
		
		int[] Feld = new int[10];
		Random rand = new Random();
		
		for (int i = 0; i < Feld.length; i++) {
			Feld[i] = rand.nextInt(10);
		}
		
		for (int i = 0; i < Feld.length; i++) {
			
		System.out.print(Feld[i]+", ");
		}
	}

}
