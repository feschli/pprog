package blabla;

import java.util.Random;

public class RandomStuff {
	public static void main(String[] args){
		Random rand = new Random();
		System.out.println(rand.nextInt());
		System.out.println(rand.nextInt());
	
	}
	
	public static boolean isPrime(int i){
		if(i==1 || i==2){
			return false;
		}
		else{
			return true;
		}
	}

}
