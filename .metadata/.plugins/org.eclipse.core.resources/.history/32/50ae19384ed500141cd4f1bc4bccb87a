package assignment3;

public class Intersection {
	// Implement
	private final Sphere sphere;
	private final double t;

	public Intersection(Sphere sphere, double t) {
		// Implement

		this.sphere = sphere;
		this.t = t;
	}
	
	public Sphere getSphere(){
		return this.sphere;
	}
	
	public double getT(){
		return this.t;
	}

}
